        {
          "dataset": "LOOTS",
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "cf5afpmxgogzkc"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH daily AS (\n    SELECT\n        DATE(updated) AS day,\n        EXTRACT(ISOYEAR FROM updated) AS iso_year,\n        EXTRACT(WEEK FROM updated) AS iso_week,\n        SUM(EXTRACT(EPOCH FROM (updated -\n               (SELECT MAX(li2.updated)\n                FROM public.loots_inverter li2\n                WHERE li2.updated < loots_inverter.updated))) / 3600 * production_power) AS daily_production,\n        SUM(EXTRACT(EPOCH FROM (updated -\n               (SELECT MAX(li2.updated)\n                FROM public.loots_inverter li2\n                WHERE li2.updated < loots_inverter.updated))) / 3600 * purchase_power)   AS daily_purchased\n    FROM public.loots_inverter\n    WHERE $__timeFilter(updated)\n    GROUP BY 1, 2, 3\n)\nSELECT\n    DATE_TRUNC('week', MIN(day)) AS time,\n    AVG(daily_production) AS production_units,\n    AVG(daily_purchased) AS purchased_units\nFROM daily\nGROUP BY iso_year, iso_week\nORDER BY time",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          },
          "table": "loots_inverter"
        }
      ],
      "title": "Produced and purchased (average per day by ISO week)",
      "type": "timeseries"
