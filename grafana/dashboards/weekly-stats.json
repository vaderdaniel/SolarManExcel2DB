          "rawQuery": true,
          "rawSql": "WITH daily AS (\n    SELECT\n        DATE(updated) AS day,\n        EXTRACT(ISOYEAR FROM updated) AS iso_year,\n        EXTRACT(WEEK FROM updated) AS iso_week,\n        SUM(EXTRACT(EPOCH FROM (updated -\n               (SELECT MAX(li2.updated)\n                FROM public.loots_inverter li2\n                WHERE li2.updated < loots_inverter.updated))) / 3600 * consume_power) AS daily_consumed,\n        SUM(EXTRACT(EPOCH FROM (updated -\n               (SELECT MAX(li2.updated)\n                FROM public.loots_inverter li2\n                WHERE li2.updated < loots_inverter.updated))) / 3600 * (-charge_power)) AS daily_charging,\n        SUM(EXTRACT(EPOCH FROM (updated -\n               (SELECT MAX(li2.updated)\n                FROM public.loots_inverter li2\n                WHERE li2.updated < loots_inverter.updated))) / 3600 * (-feed_in)) AS daily_feed_in\n    FROM public.loots_inverter\n    WHERE $__timeFilter(updated)\n    GROUP BY 1, 2, 3\n)\nSELECT\n    DATE_TRUNC('week', MIN(day)) AS time,\n    AVG(daily_consumed) AS consumed_units,\n    AVG(daily_charging) AS charging_units,\n    AVG(daily_feed_in) AS feed_in_units\nFROM daily\nGROUP BY iso_year, iso_week\nORDER BY time",
