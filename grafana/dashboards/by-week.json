          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH time_deltas AS (\n    SELECT\n        EXTRACT(WEEK FROM updated) AS iso_week,\n        DATE(updated) AS day,\n        updated,\n        LAG(updated) OVER (ORDER BY updated) AS prev_updated,\n        production_power,\n        purchase_power\n    FROM public.loots_inverter\n),\ndaily AS (\n    SELECT\n        iso_week,\n        day,\n        SUM(EXTRACT(EPOCH FROM (updated - prev_updated)) / 3600 * production_power) AS daily_production,\n        SUM(EXTRACT(EPOCH FROM (updated - prev_updated)) / 3600 * purchase_power) AS daily_purchased\n    FROM time_deltas\n    WHERE prev_updated IS NOT NULL\n    GROUP BY iso_week, day\n)\nSELECT\n    'W' || CAST(iso_week AS TEXT) AS week_number,\n    AVG(daily_production) AS production_units,\n    AVG(daily_purchased) AS purchased_units\nFROM daily\nGROUP BY iso_week\nORDER BY iso_week",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          },
          "table": "loots_inverter"
        }
      ],
      "title": "Produced and purchased (average per day by ISO week)",
