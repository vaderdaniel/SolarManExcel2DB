          "rawQuery": true,
          "rawSql": "WITH time_deltas AS (\n    SELECT\n        EXTRACT(WEEK FROM updated) AS iso_week,\n        DATE(updated) AS day,\n        updated,\n        LAG(updated) OVER (ORDER BY updated) AS prev_updated,\n        consume_power,\n        charge_power,\n        feed_in\n    FROM public.loots_inverter\n),\ndaily AS (\n    SELECT\n        iso_week,\n        day,\n        SUM(EXTRACT(EPOCH FROM (updated - prev_updated)) / 3600 * consume_power) AS daily_consumed,\n        SUM(EXTRACT(EPOCH FROM (updated - prev_updated)) / 3600 * (-charge_power)) AS daily_charging,\n        SUM(EXTRACT(EPOCH FROM (updated - prev_updated)) / 3600 * (-feed_in)) AS daily_feed_in\n    FROM time_deltas\n    WHERE prev_updated IS NOT NULL\n    GROUP BY iso_week, day\n)\nSELECT\n    'W' || CAST(iso_week AS TEXT) AS week_number,\n    AVG(daily_consumed) AS consumed_units,\n    AVG(daily_charging) AS charging_units,\n    AVG(daily_feed_in) AS feed_in_units\nFROM daily\nGROUP BY iso_week\nORDER BY iso_week",
